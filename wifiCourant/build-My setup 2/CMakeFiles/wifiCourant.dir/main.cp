 static err_t wifiesp_process(void)
 {
     uint8_t rx_buf[ 610 ] = { 0 };
     int32_t rx_size = 0;
     rx_size = wifiesp_generic_read(&wifiesp, rx_buf,  610 );
     if (rx_size > 0)
     {
         int32_t buf_cnt = app_buf_len;
         if (((app_buf_len + rx_size) >  610 ) && (app_buf_len > 0))
         {
             buf_cnt =  610  - ((app_buf_len + rx_size) -  610 );
             memmove(app_buf, &app_buf[ 610  - buf_cnt], buf_cnt);
         }
         for (int32_t rx_cnt = 0; rx_cnt < rx_size; rx_cnt++)
         {
             if (rx_buf[rx_cnt])
             {
                 app_buf[buf_cnt++] = rx_buf[rx_cnt];
                 if (app_buf_len <  610 )
                 {
                     app_buf_len++;
                 }
             }
         }
         return WIFIESP_OK;
     }
     return WIFIESP_ERROR;
 }

 static err_t wifiesp_rsp_check(char * response)
